{
    "version": 2,
    "config": {
        "runtime": "python3.8"
      },
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/main.py"
      }
    ],
    "buildCommand": "pip install -r requirements.txt",
    
  }
  