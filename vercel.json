{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python",
        "config": {
        "runtime": "python3.8"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/main.py"
      }
    ],
    "buildCommand": "pip install -r requirements.txt"
    
  }
  